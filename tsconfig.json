{
	"version": "1.8.10",
  "compilerOptions": {
    "outDir": ".",
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "noImplicitAny": true,
    "declaration": false,
    "skipLibCheck": true,
    "stripInternal": true,
    "lib": ["dom", "es6"],
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "files": [
		"typings/index.d.ts",
    "src/checklist.ts",
    "src/index.ts"
  ],
  "angularCompilerOptions": {
		"strictMetadataEmit": true
  }
}
